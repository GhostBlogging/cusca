{
  "name": "cusca",
  "description": "A clean theme for Ghost.",
  "demo": "https://wdiaz.org",
  "version": "0.1.0",
  "engines": {
    "ghost": ">=2.0.0"
  },
  "license": "MIT",
  "screenshots": {
    "desktop": "assets/screenshot-desktop.jpg",
    "mobile": "assets/screenshot-mobile.jpg"
  },
  "scripts": {
    "dev": "./node_modules/.bin/webpack --progress --colors -d --config src/build/webpack.config.js",
    "build:prod": "./node_modules/.bin/webpack --progress --colors -p --config src/build/webpack.config.js",
    "start": "./node_modules/.bin/webpack --hide-modules --colors -d --watch --config src/build/webpack.config.js",
    "test": "gscan ."
  },
  "author": {
    "name": "William Diaz",
    "email": "wdiazux@gmail.com",
    "url": "https://www.wdiaz.org"
  },
  "gpm": {
    "type": "theme",
    "categories": [
      "Minimal",
      "Magazine",
      "Personal"
    ]
  },
  "keywords": [
    "ghost",
    "theme",
    "ghost-theme",
    "responsive",
    "foundation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wdiazux/cusca.git"
  },
  "bugs": {
    "url": "https://github.com/wdiazux/cusca/issues"
  },
  "contributors": "https://github.com/wdiazux/cusca/graphs/contributors",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/webpack-env": "^1.13.6",
    "ajv": "^6.6.2",
    "autoprefixer": "^9.4.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "browserslist": "^4.3.6",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.0.1",
    "cssnano": "^4.1.7",
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.7",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.5"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions"
  ],
  "config": {
    "posts_per_page": 10
  },
  "homepage": "https://github.com/wdiazux/cusca#readme",
  "main": "gulpfile.js",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.1",
    "foundation-sites": "^6.5.1",
    "jquery": "^3.3.1",
    "particles.js": "^2.0.0",
    "roboto-fontface": "^0.10.0",
    "shufflejs": "^5.2.1",
    "what-input": "^5.1.3"
  }
}
