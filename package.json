{
  "name": "cusca",
  "description": "A clean theme for Ghost.",
  "demo": "https://wdiaz.org",
  "version": "0.1.0",
  "engines": {
    "ghost": ">=2.0.0"
  },
  "license": "MIT",
  "screenshots": {
    "desktop": "assets/screenshot-desktop.jpg",
    "mobile": "assets/screenshot-mobile.jpg"
  },
  "scripts": {
    "dev": "./node_modules/.bin/webpack --progress -d --config src/build/webpack.config.js",
    "build:prod": "./node_modules/.bin/webpack --progress -p --config src/build/webpack.config.js",
    "start": "./node_modules/.bin/webpack --hide-modules -d --watch --config src/build/webpack.config.js",
    "test": "gscan ."
  },
  "author": {
    "name": "William Diaz",
    "email": "wdiazux@gmail.com",
    "url": "https://www.wdiaz.org"
  },
  "gpm": {
    "type": "theme",
    "categories": [
      "Minimal",
      "Magazine",
      "Personal"
    ]
  },
  "keywords": [
    "ghost",
    "theme",
    "ghost-theme",
    "responsive",
    "foundation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wdiazux/cusca.git"
  },
  "bugs": {
    "url": "https://github.com/wdiazux/cusca/issues"
  },
  "contributors": "https://github.com/wdiazux/cusca/graphs/contributors",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/webpack-env": "^1.13.6",
    "ajv": "^6.5.3",
    "autoprefixer": "^9.1.3",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.24.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "browserslist": "^4.1.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.0",
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.2",
    "postcss-loader": "^3.0.0",
    "precss": "^3.1.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "typescript": "^3.0.3",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4"
  },
  "browserslist": [
    "> 5%",
    "last 2 versions"
  ],
  "config": {
    "posts_per_page": 10
  },
  "homepage": "https://github.com/wdiazux/cusca#readme",
  "main": "gulpfile.js",
  "dependencies": {
    "foundation-sites": "^6.5.0-rc.2",
    "jquery": "^3.3.1",
    "shufflejs": "^5.2.0",
    "what-input": "^5.1.1"
  }
}
